systemLog:
  destination: file
  path: <%= node['mongodb']['logPath'] + "/mongodb.log" %>
  logAppend: true
storage:
  dbPath: <%= node['mongodb']['dbPath'] %>
  journal:
    enabled: true
processManagement:
  fork: true
net:
  port: <%= node['mongodb']['port'] %>
  bindIp: <%= node['mongodb']['bindIp'] %>
